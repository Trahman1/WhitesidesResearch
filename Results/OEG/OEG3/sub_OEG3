#!/bin/bash
#SBATCH --job-name="NPT_OEG3_2_400_400"
#SBATCH --output="job.%j.%N.out"
#SBATCH --partition=shared
#SBATCH --nodes=1
#SBATCH --ntasks-per-node=12
#SBATCH -t 48:00:00
#SBATCH -A csd598
module load lammps
cd /oasis/scratch/comet/trahman2/temp_project/Aromodel/OEG3
export OMP_NUM_THREADS=1

mpirun -np 12 lammps < in.NPT_OEG3_2_400_400 > log.NPT_OEG3_2_400_400

