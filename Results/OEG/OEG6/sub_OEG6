#!/bin/bash
#SBATCH --job-name="init_OEG6"
#SBATCH --output="job.%j.%N.out"
#SBATCH --partition=shared
#SBATCH --nodes=1
#SBATCH --ntasks-per-node=12
#SBATCH -t 48:00:00
#SBATCH -A csd588
module load lammps
cd /oasis/scratch/comet/trahman2/temp_project/Aromodel/OEG6
export OMP_NUM_THREADS=1

mpirun -np 12 lammps < in.init_OEG6 > log.init_OEG6

