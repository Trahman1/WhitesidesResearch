#!/bin/bash
#SBATCH --job-name="NPT_OEG4_6_300_300"
#SBATCH --output="job.%j.%N.out"
#SBATCH --partition=shared
#SBATCH --nodes=1
#SBATCH --ntasks-per-node=12
#SBATCH -t 48:00:00
#SBATCH -A csd588
module load lammps
cd /oasis/scratch/comet/trahman2/temp_project/Aromodel/OEG4
export OMP_NUM_THREADS=1

mpirun -np 12 lammps < in.NPT_OEG4_6_300_300 > log.NPT_OEG4_6_300_300

